<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<!-- MANEJADORES / RECURSOS -->
	<application>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>es</supported-locale>
			<!-- supported-locale>en</supported-locale -->
		</locale-config>

 <message-bundle>
	  	com.equivida.intranet.recursos.default
	  </message-bundle>

		<resource-bundle>
			<base-name>com.equivida.intranet.recursos.etiquetas</base-name>
			<var>etiquetas</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.equivida.intranet.recursos.mensajes</base-name>
			<var>mensajes</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.equivida.intranet.recursos.botones</base-name>
			<var>botones</var>
		</resource-bundle>
		<!-- 
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 -->
	</application>
</faces-config>
